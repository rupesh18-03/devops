groovy
pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps { checkout scm }
        }
        stage('Install') {
            steps { bat 'npm install' }
        }
        stage('Test') {
            steps { bat 'npm test' }
        }
        stage('Deploy') {
            steps { bat 'echo "Deployed!"' }
        }
    }
}

